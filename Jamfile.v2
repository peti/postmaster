# build postmaster

exe postmaster
    : main.cpp
    : <use>/boost
      <use>/boost/program_options <library>/boost/program_options//boost_program_options
      <use>/boost/test            <library>/boost/test//boost_prg_exec_monitor
      <use>/boost/system          <library>/boost/system//boost_system
    : <threading>single
    ;
